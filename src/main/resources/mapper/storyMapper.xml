<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="storyMapper">

        <insert id="insertStory" parameterType="model.Story" useGeneratedKeys="true" keyProperty="id">
                insert into st_info (title,post_url,content)
                values (#{title},#{post_url},#{content})
        </insert>

</mapper>